<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane xmlns:fx="http://javafx.com/fxml" fx:controller="janb.controllers.Controller" alignment="center" hgap="10" vgap="10" styleClass="root">

    <padding>
        <Insets top="15" right="15" bottom="10" left="15"/>
    </padding>


    <TreeView fx:id="treeView" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <VBox fx:id="textHolder" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="1"/>

    <HBox spacing="10" alignment="bottom_right" GridPane.columnIndex="1" GridPane.rowIndex="2">
        <Button text="Dump Model" onAction="#dumpModel"/>
    </HBox>


    <stylesheets>
        <URL value="@Login.css" />
    </stylesheets>
</GridPane>