<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane xmlns:fx="http://javafx.com/fxml" fx:controller="janb.ui.StartPage" alignment="center" hgap="10" vgap="10" styleClass="root">

    <padding>
        <Insets top="15" right="15" bottom="10" left="15"/>
    </padding>


    <Button text="New Project" onAction="#newProject" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Button text="Open Project" onAction="#openProject" GridPane.columnIndex="0" GridPane.rowIndex="1" />

    <stylesheets>
        <URL value="@Login.css" />
    </stylesheets>
</GridPane>